{
  "name": "cbidb-staff-web",
  "version": "0.1.0",
  "description": "CBI Staff Application.  Theme based on AppStack React by Paul Laros <support@bootlab.io>",
  "scripts": {
    "server": "webpack --env.platform=server",
    "web": "webpack --env.platform=web",
    "sass:compile": "node-sass --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "node scripts/build.js",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "node scripts/start.js",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@sentry/browser": "^6.2.4",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "bootstrap": "^4.6.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chokidar-cli": "^2.1.0",
    "css-loader": "4.3.0",
    "file-loader": "6.1.1",
    "fp-ts": "^1.18.1",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "4.5.0",
    "io-ts": "^1.8.6",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "0.11.3",
    "moment": "^2.29.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.2",
    "react-redux-toastr": "^7.6.5",
    "react-refresh": "^0.8.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "redux": "^4.0.5",
    "resolve": "1.18.1",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.0.5",
    "style-loader": "1.3.0",
